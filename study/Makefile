DIRS =  orig \
	event_fifo \
	event_fifo_asynch_1 \
	event_sel_asynch_1
all:
	(cd orig; make)
	(cd event_fifo; make)
	(cd event_fifo_asynch_1; make)
	(cd event_sel_asynch_1; make)


clean:
	rm -f *~ \#*\#
	(cd orig; make clean)
	(cd event_fifo; make clean)
	(cd event_fifo_asynch_1; make clean)
	(cd event_sel_asynch_1; make clean)

